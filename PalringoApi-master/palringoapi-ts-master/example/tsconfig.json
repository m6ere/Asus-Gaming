{
    "compilerOptions": {
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": true,
      "sourceMap": false,
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "declaration": true,
      "lib": [
        "es2018",
        "dom"
      ],
      "outDir": "./typings",
      "allowUnreachableCode": true,
      "types": [ "node", "socket.io-client" ],
      "typeRoots": ["./node_modules/@types"],
      "experimentalDecorators": true
    },
    "include": ["src"]
  }
  